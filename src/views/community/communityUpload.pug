extends ../base

block content 
  form.form__post(method="post")
    if errorMessage 
      span=errorMessage
    input.form__post-space(name="title" type="text" required placeholder="Title")
    select(name="division")
      option(value="곤지암") 곤지암
      option(value="하이원") 하이원
      option(value="휘닉스") 휘닉스
      option(value="용평") 용평
      option(value="무주") 무주
      option(value="에덴밸리") 에덴밸리
      option(value="웰리힐리") 웰리힐리
      option(value="오투") 오투
      option(value="지산") 지산
      option(value="비발디") 비발디
      option(value="알펜시아") 알펜시아
      option(value="오크밸리") 오크밸리
      option(value="양지파인") 양지파인
      option(value="엘리시안") 엘리시안
    textarea(name="contents" type="text" required placeholder="Description")
    input(type="submit" value="Upload")